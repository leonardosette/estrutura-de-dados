void main(){
    int i = 0;
    int vetor [10];
//inicialização da memoria
    for (i = 0; i<10; i++){
        vetor[i] = 0;
// preenchimento do vetor
    for (i = 0; i<10;i++){
        printf("Digite o valor da posicao %d: ", i);
        scanf("%d", &vetor[i]);
 }
    for (i = 0; i<10;i++){
            printf("vetor[%d] = %d \n", i, vetor[i]);
    }

}
}
